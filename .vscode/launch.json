{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Node Functions",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "preLaunchTask": "func: host start"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug sandbox",
      "program": "index",
      "args": ["${input:sandboxCodeSelection}"],
      "cwd": "${workspaceRoot}/sandbox"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug DIRECT doHealthCheck",
      "program": "index",
      "args": ["6:doHealthCheck"],
      "cwd": "${workspaceRoot}/sandbox"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug DIRECT doCovidStateDashboarV2",
      "program": "index",
      "args": ["7:doCovidStateDashboarV2"],
      "cwd": "${workspaceRoot}/sandbox"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug DIRECT CovidEquityData",
      "program": "index",
      "args": ["1:CovidEquityData"],
      "cwd": "${workspaceRoot}/sandbox"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug DIRECT CovidNewsFeed",
      "program": "index",
      "args": ["5:CovidNewsFeed"],
      "cwd": "${workspaceRoot}/sandbox"
    },
  ],
  "inputs": [
    {
      "id": "sandboxCodeSelection",
      "type": "pickString",
      "options": [
        "1:CovidEquityData",
        "2:doDailyStatsPr",
        "3:doTranslationPrUpdate",
        "4:doWeeklyUpdatePrs",
        "5:CovidNewsFeed",
        "6:doHealthCheck",
        "7:doCovidStateDashboarV2"
      ],
      "description": "Select code to debug"
    }
  ]
}