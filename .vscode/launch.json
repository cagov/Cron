{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Node Functions",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "preLaunchTask": "func: host start"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug sandbox",
      "program": "index",
      "args": ["${input:sandboxCodeSelection}"],
      "cwd": "${workspaceRoot}/sandbox"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug DIRECT doHealthCheck",
      "program": "index",
      "args": ["6:doHealthCheck"],
      "cwd": "${workspaceRoot}/sandbox"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug DIRECT doCovidStateDashboarV2",
      "program": "index",
      "args": ["7:doCovidStateDashboarV2"],
      "cwd": "${workspaceRoot}/sandbox"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug DIRECT CovidEquityData",
      "program": "index",
      "args": ["1:CovidEquityData"],
      "cwd": "${workspaceRoot}/sandbox"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug DIRECT doTranslationPrUpdate",
      "program": "index",
      "args": ["3:doTranslationPrUpdate"],
      "cwd": "${workspaceRoot}/sandbox"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug DIRECT doWeeklyUpdatePrs",
      "program": "index",
      "args": ["4:doWeeklyUpdatePrs"],
      "cwd": "${workspaceRoot}/sandbox"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug DIRECT CovidVaccineEquity",
      "program": "index",
      "args": ["8:doCovidVaccineEquity"],
      "cwd": "${workspaceRoot}/sandbox"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug DIRECT CovidVaccineHPIV2",
      "program": "index",
      "args": ["10:doCovidVaccineHPIV2"],
      "cwd": "${workspaceRoot}/sandbox"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug DIRECT AutoApprover",
      "program": "index",
      "args": ["12:AutoApprover"],
      "cwd": "${workspaceRoot}/sandbox"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug DIRECT CovidStateDashboardTables",
      "program": "index",
      "args": ["13:CovidStateDashboardTables"],
      "cwd": "${workspaceRoot}/sandbox"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug DIRECT TEMP",
      "program": "index",
      "args": ["temp:TEMP"],
      "cwd": "${workspaceRoot}/sandbox"
    },
  ],
  "inputs": [
    {
      "id": "sandboxCodeSelection",
      "type": "pickString",
      "options": [
        "1:CovidEquityData",
        "2:doDailyStatsPr",
        "3:doTranslationPrUpdate",
        "4:doWeeklyUpdatePrs",
        "6:doHealthCheck",
        "7:doCovidStateDashboarV2",
        "8:doCovidVaccineEquity",
        "9:doCovidVaccineHPI",
        "10:doCovidVaccineHPIV2"
      ],
      "description": "Select code to debug"
    }
  ]
}