              "type": "integer",
              "minimum": 0
            },
            {
              "type": "null"
            }
          ]
        },
        "NOT_MISSING": {
          "oneOf": [
            {
              "type": "integer",
              "minimum": 0
            },
            {
              "type": "null"
            }
          ]
        },
        "TOTAL": {
          "oneOf": [
            {
              "type": "integer",
              "minimum": 0
            },
            {
              "type": "null"
            }
          ]
        },
        "PERCENT_COMPLETE": {
          "oneOf": [
            {
              "type": "number",
              "minimum": 0,
              "maximum": 1
            },
            {
              "type": "null"
            }
          ]
        },
        "PERCENT_COMPLETE_30_DAYS_PRIOR": {
          "oneOf": [
            {
              "type": "number",
              "minimum": 0,
              "maximum": 1
            },
            {
              "type": "null"
            }
          ]
        },
        "PERCENT_COMPLETE_30_DAYS_DIFF": {
          "oneOf": [
            {
              "type": "number"
            },
            {
              "type": "null"
            }
          ]
        },
        "REPORT_DATE": {
          "$ref": "#/definitions/date_YYYY_MM_DD"
        }
      }
    }
  },
